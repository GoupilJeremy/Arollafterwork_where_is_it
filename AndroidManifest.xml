<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.arolla.afterwork.whereisit" android:versionCode="1" android:versionName="1.0" >
    
    <uses-sdk android:minSdkVersion="8" 
        android:targetSdkVersion="14"/>
    
    <uses-permission android:name="android.permission.INTERNET"/>
    
    <application 
        android:icon="@drawable/where_is_it_icon" 
        android:label="@string/app_name"
        android:name=".WhereIsItApplication">
        
    	<uses-library android:name="com.google.android.maps"/>
        
        <activity android:name="WelcomeActivity" android:label="@string/activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ShowPhotoActivity" android:label="@string/activity_main">
            <intent-filter>
                <action android:name="ShowPhoto" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!-- activity android:name=".WhereIsItActivity" android:label="@string/activity_where_is_it">
            <intent-filter>
                <action android:name="WhereIsIt" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".ResultActivity" android:label="@string/activity_result" >
            <intent-filter>
                <action android:name="Result" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity-->
    </application>
</manifest>